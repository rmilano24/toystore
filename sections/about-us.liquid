  {% if section.settings.show_team_section != blank %}
  <div class="grid-uniform section-seven">
    <div class="container">
      {% if section.settings.section_7_mainheading != blank %}
      <div class="main-heading">
        <h2>  {{section.settings.section_7_mainheading}}</h2>
        {% if section.settings.section_7_subheading != blank %}
        <h6>  {{section.settings.section_7_subheading}}</h6>
        {% endif %}
      </div>
      {% endif %}
      <div class="grid__item">
        {% if section.blocks.size > 0 %}  
        {% for block in section.blocks %}         
        <div class="grid__item wide--one-quarter post-large--one-quarter large--one-quarter medium--one-half small-grid__item">
          <div class="team-wrapper">
            {% if block.settings.team_6_image != blank %}
            <div class="team-img">
              <img src="{{ block.settings.team_6_image | img_url: '1024x1024' }}" alt="" title="" /> 
            </div>
            {% endif %}
            <div class="team-detail">
              {% if block.settings.team_title != blank %}
              <h5>{{block.settings.team_title }}</h5>
              {% endif %}
              {% if block.settings.team_subtitle != blank %}
              <h6>{{block.settings.team_subtitle }}</h6>
              {% endif %}
              <ul class="social-icon">
                {% if block.settings.social_1_icon != blank %}
                <li><a href=""><i class="{{block.settings.social_1_icon }}" aria-hidden="true"></i></a></li>
                {% endif %}
                {% if block.settings.social_2_icon != blank %}
                <li><a href=""><i class="{{block.settings.social_2_icon }}" aria-hidden="true"></i></a></li>
                {% endif %}
                {% if block.settings.social_3_icon != blank %}
                <li><a href=""><i class="{{block.settings.social_3_icon }}" aria-hidden="true"></i></a></li>
                {% endif %}
                {% if block.settings.social_4_icon != blank %}
                <li><a href=""><i class="{{block.settings.social_4_icon }}" aria-hidden="true"></i></a></li>
                {% endif %}
              </ul> 
            </div>
          </div>
        </div>        
        {% endfor %}
        {% endif %}
      </div>
    </div>
  </div>
  {% endif %}

  {% if section.settings.showvedaone_6_section %}
  <div class="grid-uniform section-six"  style="background-color:{{section.settings.aboutveda_1_sectionsixbg}};">
    <div class="container">   
      <div class="grid__item wide--three-fifths post-large--three-fifths">     
        <div class="sectionsix-txtblk">
          <div class="sectionsix-img">
            {% if  section.settings.abtone_image_6 != blank %}
            <img src="{{  section.settings.abtone_image_6 | img_url: "master" }}" alt="{{ section.settings.abtone_image_6_heading }}" />
            {% endif %} 
            {% if  section.settings.abtone_6_des != blank %}
            <p style="color:{{section.settings.abtone_6_desc}}">{{section.settings.abtone_6_des }}</p>
            {% endif %} 
          </div>
        </div>  
      </div>
      <div class="grid__item wide--two-fifths post-large--two-fifths">
        {% include 'newsletter-form' %}
      </div>
    </div>
  </div>
  {% endif %}
</div>





























<style>


  .aboutus .author-name::after {background:{{section.settings.aboutveda_1_bordercolor}};}

  .aboutus  .detail-block:hover .detail.detail-one{background-color:{{ section.settings.detail_1_bgcolor | color_modify: 'alpha', 1 }}!important;}

  .aboutus  .detail-block:hover .detail.detail-two{background-color:{{ section.settings.detail_2_bgcolor | color_modify: 'alpha', 1 }}!important;}

  .aboutus  .detail-block:hover .detail.detail-three{background-color:{{ section.settings.detail_3_bgcolor | color_modify: 'alpha', 1 }}!important;}

  .aboutus  .detail-block:hover .detail.detail-four{background-color:{{ section.settings.detail_4_bgcolor | color_modify: 'alpha', 1 }}!important;}

  .aboutus .icon-wrapper .icon {border: 5px solid {{section.settings.section_2_bordercolor}};}

  .aboutus .icon-wrapper h5 ,.aboutus .icon-wrapper span {color: {{section.settings.section_2_iconcolor}};}

  .aboutus .section-seven .main-heading h2{color:{{section.settings.mainheading_color}};}

  .aboutus  .section-seven .main-heading  h6{color:{{section.settings.subheading_color}};}

  .aboutus  .team-detail{background:{{section.settings.section_7_teambgcolor}};}

  .aboutus    .team-detail  h5 , .aboutus .team-detail  h6{color:{{section.settings.section_7_teamcolor}};}

  .aboutus   .social-icon a{border:1px solid {{section.settings.section_7_teamcolor}};color:{{section.settings.section_7_teamcolor}};}

  .aboutus  .social-icon a:hover{border:1px solid {{section.settings.section_7_teambgcolor}};}

  .aboutus .section-six .btn { background:{{section.settings.aboutveda_1_pribtnbg}};color:{{section.settings.aboutveda_1_pribtncolor}};}

  .aboutus .section-six .btn:hover{background:{{section.settings.aboutvedabtnhoverbg}};color:{{section.settings.aboutvedabtnhovercolor}} !important;}


</style>
















