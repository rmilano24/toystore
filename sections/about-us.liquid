            {% if section.settings.icon_3_img  %}
            <div class="icon">
              <img src="{{  section.settings.icon_3_img  | img_url: "master" }}" alt="{{ section.settings.icon_3_imgheading }}" />
            </div>
            {% endif %}
            {% if  section.settings.icon_3_title  != blank %}
            <h5>{{section.settings.icon_3_title}}</h5>
            {% endif %}
          </div>
        </div>
        {% endif %} 
        {% if section.settings.show_4_icon  %}
        <div class="column grid__item wide--one-fifth post-large--one-fifth large--one-fifth medium--one-third small-grid__item  small--grid__item">
