
<div data-section-id="{{ section.id }}"  data-section-type="About section" class="aboutus">
  {% if section.settings.showvedaone_1_section %}
  <div class="grid-uniform section-one">
    <div class="container">
      <div class="grid__item">
        <div class="grid__item post-large--one-half">
          {% if section.settings.abtone_1_title != blank %}
          <div class="main-title">
            <h2 style="color:{{section.settings.abtone_1_maintilte }}">{{ section.settings.abtone_1_title }}</h2>
            <div class="title-sep" style="background:{{section.settings.abtone_1_maintilte }}"></div>
          </div>
          {% endif %}
          {% if section.settings.abtone_1_des != blank %}
          <p style="color:{{section.settings.abtone_1_desc }}">{{section.settings.abtone_1_des }}</p>
          {% endif %}          
          {% if section.settings.abtone_1_quote != blank %}
          <div class="quote">
            <p style="color:{{section.settings.abtone_1_quotecolor }}"> {{section.settings.abtone_1_quote }} </p>  
            {% if section.settings.abtone_1_authorname!= blank %}    <span class="author-name" style="color:{{section.settings.abtone_1_authorcolor }}"> {{section.settings.abtone_1_authorname}}</span>{% endif %}
          </div>
          {% endif %}
        </div>
        {% if  section.settings.abtone_image_1 != blank %}
        <div class="grid__item post-large--one-half">
          <div class="section-one-rightimg">
            <img src="{{  section.settings.abtone_image_1 | img_url: "master" }}" alt="{{ section.settings.abtone_image_1_heading }}" />
          </div>
        </div>
        {% endif %} 
      </div>
    </div>
  </div>
  {% endif %} 


  {% if section.settings.show_2_section %}
  <div class="section-two">
    <div class="container">
      <div class="grid__item">
        <div class="grid__item">
          {% if section.settings.detail_1_section %}
          <div class="grid__item wide--one-half post-large--one-half large--one-half">
            <div class="detail-block" style="background-image:url({{section.settings.detail_1_bgimg | img_url: '2048x2048' }});">
              <div class="grid__item wide--one-half post-large--one-half large--one-half">
                <div class="detail detail-one" style="background-color:{{ section.settings.detail_1_bgcolor | color_modify: 'alpha', 0.8 }};" >
                  {% if section.settings.detail_1_icon != blank %}
                  <span class="detail-icon">
                    <i class="{{section.settings.detail_1_icon }}"   style="color:{{section.settings.detailicon_1_color}};background:{{section.settings.detailicon_1_bgcolor}};" aria-hidden="true"></i>
                  </span>
                  {% endif %} 
                  {% if section.settings.detail_1_title != blank %}
                  <h4 style="color:{{section.settings.detail_1_title_color}};">{{section.settings.detail_1_title }}</h4>
                  {% endif %}
                  {% if section.settings.detail_1_desc != blank %}
                  <p style="color:{{section.settings.detail_1_desc_color}};">{{section.settings.detail_1_desc}}</p>
                  {% endif %} 
                  {% if section.settings.detail_1_price != blank %}
                  <span class="price" style="color:{{section.settings.detail_1_price_color}};background:{{section.settings.detail_1_price_bgcolor}};">{{section.settings.detail_1_price}}</span>
                  {% endif %}                  
                </div>
              </div>
            </div>            
          </div>
          {% endif %}
          {% if section.settings.detail_2_section %}
          <div class="grid__item wide--one-half post-large--one-half large--one-half">
            <div class="detail-block " style="background-image:url({{section.settings.detail_2_bgimg | img_url: '2048x2048' }});">
              <div class="grid__item wide--one-half post-large--one-half large--one-half">
                <div class="detail detail-two" style="background-color:{{ section.settings.detail_2_bgcolor | color_modify: 'alpha', 0.8 }};" >
                  {% if section.settings.detail_2_icon != blank %}
                  <span class="detail-icon">
                    <i class="{{section.settings.detail_2_icon }}"   style="color:{{section.settings.detailicon_2_color}};background:{{section.settings.detailicon_2_bgcolor}};" aria-hidden="true"></i>
                  </span>
                  {% endif %} 
                  {% if section.settings.detail_2_title != blank %}
                  <h4 style="color:{{section.settings.detail_2_title_color}};">{{section.settings.detail_2_title }}</h4>
                  {% endif %}
                  {% if section.settings.detail_2_desc != blank %}
                  <p style="color:{{section.settings.detail_2_desc_color}};">{{section.settings.detail_2_desc}}</p>
                  {% endif %} 
                  {% if section.settings.detail_2_price != blank %}
                  <span class="price" style="color:{{section.settings.detail_2_price_color}};background:{{section.settings.detail_2_price_bgcolor}};">{{section.settings.detail_2_price}}</span>
                  {% endif %}                 
                </div>
              </div>
            </div>
          </div>
          {% endif %}
          {% if section.settings.detail_3_section %}
          <div class="grid__item wide--one-half post-large--one-half large--one-half">
            <div class="detail-block" style="background-image:url({{section.settings.detail_3_bgimg | img_url: '2048x2048' }});">
              <div class="grid__item wide--one-half post-large--one-half large--one-half"></div>
              <div class="grid__item wide--one-half post-large--one-half large--one-half">
                <div class="detail detail-three" style="background-color:{{ section.settings.detail_3_bgcolor | color_modify: 'alpha', 0.8 }};" >
                  {% if section.settings.detail_3_icon != blank %}
                  <span class="detail-icon">
                    <i class="{{section.settings.detail_3_icon }}"   style="color:{{section.settings.detailicon_3_color}};background:{{section.settings.detailicon_3_bgcolor}};" aria-hidden="true"></i>
                  </span>
                  {% endif %} 
                  {% if section.settings.detail_3_title != blank %}
                  <h4 style="color:{{section.settings.detail_3_title_color}};">{{section.settings.detail_3_title }}</h4>
                  {% endif %}
                  {% if section.settings.detail_3_desc != blank %}
                  <p style="color:{{section.settings.detail_3_desc_color}};">{{section.settings.detail_3_desc}}</p>
                  {% endif %} 
                  {% if section.settings.detail_3_price != blank %}
                  <span class="price" style="color:{{section.settings.detail_3_price_color}};background:{{section.settings.detail_3_price_bgcolor}};">{{section.settings.detail_3_price}}</span>
                  {% endif %}                 
                </div>
              </div>
            </div>
          </div>
          {% endif %}
          {% if section.settings.detail_4_section %}
          <div class="grid__item wide--one-half post-large--one-half large--one-half">
            <div class="detail-block " style="background-image:url({{section.settings.detail_4_bgimg | img_url: '2048x2048' }});">
              <div class="grid__item wide--one-half post-large--one-half large--one-half"></div>
              <div class="grid__item wide--one-half post-large--one-half large--one-half">
                <div class="detail detail-four detail-right" style="background-color:{{ section.settings.detail_4_bgcolor | color_modify: 'alpha', 0.8 }};" >
                  {% if section.settings.detail_4_icon != blank %}
                  <span class="detail-icon">
                    <i class="{{section.settings.detail_4_icon }}"   style="color:{{section.settings.detailicon_4_color}};background:{{section.settings.detailicon_4_bgcolor}};" aria-hidden="true"></i>
                  </span>
                  {% endif %} 
                  {% if section.settings.detail_4_title != blank %}
                  <h4 style="color:{{section.settings.detail_4_title_color}};">{{section.settings.detail_4_title }}</h4>
                  {% endif %}
                  {% if section.settings.detail_4_desc != blank %}
                  <p style="color:{{section.settings.detail_4_desc_color}};">{{section.settings.detail_4_desc}}</p>
                  {% endif %} 
                  {% if section.settings.detail_4_price != blank %}
                  <span class="price" style="color:{{section.settings.detail_4_price_color}};background:{{section.settings.detail_4_price_bgcolor}};">{{section.settings.detail_4_price}}</span>
                  {% endif %}                 
                </div>
              </div>
            </div>
          </div>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
  {% endif %}

  {% if section.settings.section_two %}
  <div class="grid-uniform section-three" style="background:{{section.settings.section_2_bg}}">
    <div class="container">
      {% if  section.settings.section_2_title  != blank %}
      <div class="main-title">
        <h2 style="color:{{section.settings.section_2_title_color  }}">{{section.settings.section_2_title}}</h2>
        {% if  section.settings. section_2_subtitle  != blank %}
        <h6 style="color:{{section.settings.section_2_subtitle_color}}">{{section.settings.section_2_subtitle}}</h6>
        {% endif %}
        {% if  section.settings.section_2_img  != blank %}
        <img src="{{  section.settings.section_2_img  | img_url: "master" }}" alt="{{ section.settings.section_2_imgheading }}" />
        {% endif %}
      </div>
      {% endif %}
      <div class="grid__item">
        {% if section.settings.show_1_icon  %}
        <div class="column grid__item wide--one-fifth post-large--one-fifth large--one-fifth medium--one-third small-grid__item  small--grid__item">
          <div class="icon-wrapper">
            {% if section.settings.icon_1_img  %}
            <div class="icon">
              <img src="{{  section.settings.icon_1_img  | img_url: "master" }}" alt="{{ section.settings.icon_1_imgheading }}" />
            </div>
            {% endif %}
            {% if  section.settings.icon_1_title  != blank %}
            <h5>{{section.settings.icon_1_title}}</h5>
            {% endif %}
          </div>
        </div>
        {% endif %} 
        {% if section.settings.show_2_icon  %}
        <div class="column grid__item wide--one-fifth post-large--one-fifth large--one-fifth medium--one-third small-grid__item  small--grid__item">
          <div class="icon-wrapper">
            {% if section.settings.icon_2_img  %}
            <div class="icon">
              <img src="{{  section.settings.icon_2_img  | img_url: "master" }}" alt="{{ section.settings.icon_2_imgheading }}" />
            </div>
            {% endif %}
            {% if  section.settings.icon_2_title  != blank %}
            <h5>{{section.settings.icon_2_title}}</h5>
            {% endif %}
          </div>
        </div>
        {% endif %} 
        {% if section.settings.show_3_icon  %}
